{
    "name": "spatie/laravel-http-logger",
    "description": "A Laravel package to log HTTP requests",
    "keywords": [
        "spatie",
        "laravel-http-logger"
    ],
    "homepage": "https://github.com/spatie/laravel-http-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Roose",
            "email": "brent@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^9.0|^10.0|^11"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9|^10"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\HttpLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\HttpLogger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\HttpLogger\\HttpLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
